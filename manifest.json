{
  "manifest_version": 2,
  "name": "New Tab Extension",
  "version": "1.0",
  "description": "Custom New Tab Page Background",
  "icons": {
    "48": "icon.png"
  },
"permissions": [
    "storage",
    "activeTab"
],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["about:newtab"],
      "js": ["content.js"]
    }
  ]
}
